{
    "name": "top-homepage",
    "version": "1.0.0",
    "description": "A homepage project for The Odin Project demonstrating the usage of responsive design",
    "homepage": "https://github.com/RobLaughlin/TOP-Homepage#readme",
    "bugs": {
        "url": "https://github.com/RobLaughlin/TOP-Homepage/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/RobLaughlin/TOP-Homepage.git"
    },
    "license": "ISC",
    "author": "Rob Laughlin",
    "type": "module",
    "main": "babel.config.js",
    "scripts": {
        "test": "jest --verbose",
        "watch": "jest --watch --verbose /*.js",
        "build": "webpack --config webpack.prod.js",
